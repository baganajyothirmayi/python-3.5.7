    "### prime functions\n",
    "\n",
    "def prime(n):\n",
    "    c=0\n",
    "    for i in range(2,(n//2)+1):\n",
    "        if n%i==0:\n",
    "            c=1\n",
    "    if c==0:\n",
    "        print(\"prime\")\n",
    "    else:\n",
    "        print(\"not a prime\")\n",
    "n=int(input())\n",
    "prime(n)"
   ]
  }
