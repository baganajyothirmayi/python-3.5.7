    "### factorial\n",
    "\n",
    "def factorial(n):\n",
    "    fact=1\n",
    "    for i in range(1,n+1):\n",
    "        fact=fact*i\n",
    "    print(\"factorial\",fact)\n",
    "n=int(input())\n",
    "factorial(n)"
